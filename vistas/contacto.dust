<!doctype html>
<html lang="es">
	<head>
		<!-- esto es para utf-8 -->
		<meta charset="utf-8"/>
		<title>Mi pagina</title>
		
		<link href='http://fonts.googleapis.com/css?family=Monda' rel='stylesheet' type='text/css'>
		<link href="imagenes/chela.png" rel="shortcut icon"/>
		<link href="css/normalize.css" rel="stylesheet"/>				
		<link href="css/estilos.css" rel="stylesheet"/>
		
		<style>		
		</style>
	</head>
	<body>
		<header>
			<nav>
				<ul id="menu">
					<li>
						Inicio
					</li>
					<li>
						Productos
					</li>
					<li>
						Blog
					</li>
					<li>
						Acerca
					</li>
				</ul>
			</nav>
		</header>
		<form action='/Enviardatos' method="post">
					<label for="email">nombre:</label>
					<input id="email" type="text" name="nombre"/>
					<label for="email">email:</label>
					<input id="email" type="email" name="email" required/>
					<label for="email">website:</label>
					<input id="email" type="url" pattern="http(s)?://[a-zA-Z]+.[a-zA-Z] +" name="website"/>
					<label for="email">edad:</label>
					<input id="email" type="number" name="edad" min="18" max="99" step="5"/>
					<label for="email">texto:</label>
					<input id="email" type="text" name="texto"/>
					<input type="submit" value="Enviar">
				</form>
		
			
		<footer>
			<div id="derechos">
				<span>MiPagina</span> derechos reservados
			</div>
			<div id="suscripcion">
				
				<form action='/sucribir' method="post">
					<label for="email">Suscribete:</label>
					<input id="email" type="text" name="email"/>
					<input type="submit" value="Enviar">
				</form>
				
			</div>
			<section id="redes">
			<div id="google">
				<img src="imagenes/googleplus.png"/>
				
			</div>
			<div id="face">
				<img src="imagenes/facebook.png"/>
				
			</div>
			<div id="you">
				<img src="imagenes/youtube.png"/>
				
			</div>
			<div id="twi">
				<img src="imagenes/twitter.png"/>
				
			</div>
		</section>
		</footer>
	</body>
</html>










